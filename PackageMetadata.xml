<?xml version="1.0" encoding="utf-8"?>
<PropertyGroup>
	<TargetFramework>netstandard2.0</TargetFramework>
	<Title>Shadowgem</Title>
	<Authors>Tomáš Bouda</Authors>
	<PackageDescription>Library that provides extension and helper methods of all kinds.</PackageDescription>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<Version>2.0.0</Version>
	<PackageProjectUrl>https://github.com/TomasBouda/Shadowgem</PackageProjectUrl>
	<PackageIconUrl>https://cdn.rawgit.com/TomasBouda/Shadowgem/0bc7f080/TomLabs.Shadowgem/shadowgem.jpg?raw=true</PackageIconUrl>
	<RepositoryUrl>https://github.com/TomasBouda/Shadowgem</RepositoryUrl>
	<PackageTags>extension helper methods utility library</PackageTags>
	<RepositoryType>git</RepositoryType>
	<Copyright>Copyright 2018 TomLabs</Copyright>
	<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
</PropertyGroup>